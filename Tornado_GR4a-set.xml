   <key n="123">
   <name>Shift-{</name>
   <desc>Autostop</desc>
   <binding> 
    <command>nasal</command>
    <script>    
  	 gui.popupTip("Autostopping!");
     autoassist.enginestart.stop(1);			 
     autoassist.enginestart.stop(2);			 
	</script>
   </binding>
   <binding>
    <mod-ctrl>
	</mod-ctrl>
   </binding>
  </key>
  
  <key n="125">
   <name>Shift-]</name>
   <desc>Full Autostart</desc>
   <binding> 
    <command>nasal</command>
    <script>    
  	 gui.popupTip("Autostarting!");
	 settimer( func {
         autoassist.enginestart.go(1);			 
	     },0.5);
	 settimer( func {
         autoassist.enginestart.go(2);			 
		},26);
	</script>
   </binding>
   <binding>
   </binding>
  </key>
    
