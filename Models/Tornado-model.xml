<!-- A wrapper file for the T-38 3D model -->

<PropertyList>
 <path>Tornado.ac</path>
 
 <offsets>
   <x-m>8.0</x-m>
   <z-m>0</z-m>
   <pitch-deg>0.0</pitch-deg>
 </offsets>
 
 
  <!-- Burner Flame -->

 <animation>
  <type>select</type>
  <object-name>outerFlame.001</object-name>
  <object-name>InnerFlameR.001</object-name>
  <condition>
  <and>
      <property>engines/engine[0]/running</property>
      <greater-than>
    <property>engines/engine[0]/n2</property>
	<value>98.0</value>
   </greater-than>
  </and>
  </condition>
</animation>


 <animation>
  <type>select</type>
  <object-name>outerFlame</object-name>
  <object-name>InnerFlameR</object-name>
  <condition>
  <and>
      <property>engines/engine[1]/running</property>
      <greater-than>
    <property>engines/engine[1]/n2</property>
	<value>98.0</value>
   </greater-than>
  </and>
  </condition>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>InnerFlameR</object-name>
  <property>engines/engine[1]/n2</property>
  <factor>20</factor>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

<animation>
  <type>textranslate</type>
  <object-name>InnerFlameR.001</object-name>
  <property>engines/engine[1]/n2</property>
  <factor>20</factor>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
