 <animation>
  <type>pick</type>
     <object-name>ptt.fire.l</object-name>
  <object-name>ptt.fire.l.lamplens</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
     <command>nasal</command>
	 <script>avionics.firelptt.test(1)</script>
    </binding>
	 <mod-up>
	  <binding>
	  <command>nasal</command>
	  <script>avionics.firelptt.test(0)</script>
	 </binding>
	 </mod-up>
   </action>
 </animation>
 
 <animation>
  <type>translate</type>
  <object-name>ptt.fire.l</object-name>
  <object-name>ptt.fire.l.lamplens</object-name>
  <property>/instrumentation/PTT-indicators/fire-left/test</property>
  <factor>0.004</factor>
  <offset-m>0</offset-m>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>material</type>
  <object-name>ptt.fire.l.lamplens</object-name>
  <emission>
    <red>1</red>
    <green>1</green>
    <blue>1</blue>
    <factor-prop>/instrumentation/PTT-indicators/fire-left/lit</factor-prop>
  </emission>
 </animation>
 
