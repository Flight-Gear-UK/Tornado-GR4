<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

 <path>cockpit-floods.ac</path>
 
 <animation>
  <type>light</type>
  <light-type>spot</light-type>
  <object-name>cockpitfloods.1</object-name>
		<nopreview/>
		<direction><x>-0.95</x><y>-0.25</y><z>-0.575</z></direction>
		<position><x>0.4</x><y>0</y><z>0.93</z></position>
 		<ambient><r>0.2</r><g>0.0</g><b>0.0</b><a>1</a></ambient>
		<diffuse><r>0.4</r><g>0.2</g><b>0.2</b><a>1</a></diffuse>
		<specular><r>0.7</r><g>0.35</g><b>0.35</b><a>1</a></specular>
		<attenuation><c>0.8</c><l>0.07</l><q>0</q></attenuation>
		<exponent>45.0</exponent>
		<cutoff>20</cutoff>
		<near-m>0.1</near-m>
		<far-m>8</far-m>
		<dim-factor>
		<expression>
         <product>
          <value>-1</value>
          <sum>
            <product>
              <max>
                <pow>
                  <cos><property>/sim/time/sun-angle-rad</property></cos>
                  <value>0.3</value>
                </pow>
                <value>0</value>
              </max>
			  <value>1</value>
            </product>
            <value>-1</value>
          </sum>
		  <property>/systems/electrical/outputs/internal-lighting/pilot/flood-lights</property>
        </product>
      </expression>
	 </dim-factor>
 </animation>
 
