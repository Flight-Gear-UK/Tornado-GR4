
<PropertyList>

 <path>rapid-takeoff-panel.ac</path>
 
 <effect>
  <inherits-from>Effects/model-transparent</inherits-from>
  <condition>
   <property>sim/rendering/rembrandt/enabled</property>
  </condition>
  <object-name>rto-labels</object-name>
 </effect>
 
 <animation>
  <type>material</type>
  <object-name>rto-labels</object-name>
  <emission>
    <red>1</red>
    <green>0.3</green>
    <blue>0.3</blue>
    <factor-prop>/systems/electrical/outputs/internal-lighting/pilot/console-lighting</factor-prop>
  </emission>
 </animation>
 
 <animation>
  <object-name>switch.battmstr</object-name>
  <type>rotate</type>
  <property>/controls/switches/pilot/rapid-takeoff-panel/battery-master</property>
  <factor>-50</factor>
  <offset-deg>25</offset-deg>
  <center>
   <x-m>0.00589</x-m>
   <y-m>-0.0383</y-m>
   <z-m>0.03975</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>pick</type>
   <object-name>switch.battmstr</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
     <command>nasal</command>
	 <script>avionics.battmstrswitch.toggle()</script>
    </binding>
   </action>
 </animation>
 
