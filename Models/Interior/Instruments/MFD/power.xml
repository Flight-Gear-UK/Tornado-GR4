<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>power.ac</path>

	<animation>
		<type>translate</type>
		<object-name>throttle.R</object-name>
		<property>controls/engines/engine[0]/throttle</property>
		 <condition>
			 <not>
			     <property>/controls/engines/engine[0]/reverser</property>
			 </not>
		 </condition>
		<factor>0.17</factor><!-- 0.2210 (-.0511) = 0.17-->
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>throttle.L</object-name>
		<property>controls/engines/engine[1]/throttle</property>
		 <condition>
			 <not>
			     <property>/controls/engines/engine[1]/reverser</property>
			 </not>
		 </condition>
		<factor>0.17</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<object-name>throttles</object-name>
		<type>noshadow</type>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>throttle.R</object-name>
		<property>controls/engines/engine[0]/throttle</property>
		 <condition>
			     <property>/controls/engines/engine[0]/reverser</property>
		 </condition>
		<factor>0.22</factor><!-- 130% = 0.2210 -->
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>throttle.L</object-name>
		<property>controls/engines/engine[1]/throttle</property>
		 <condition>
			     <property>/controls/engines/engine[1]/reverser</property>
		 </condition>
		<factor>0.22</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

