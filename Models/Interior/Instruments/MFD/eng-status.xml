<?xml version="1.0"?>
<PropertyList>

 <path>eng-status.ac</path>
 

 <animation>
	 <type>select</type>
	 <object-name>EngEstatus</object-name>
	 <!--
	 <condition>   
		 <equals>
			 <property>controls/HSD/on-off</property>
			 <value>0</value>
		 </equals>
	 </condition>
	 -->
 </animation>


	<animation>
		<type>rotate</type>
		<object-name>rpm.L</object-name>
		<property>engines/engine[0]/rpm</property>
		<factor>2</factor>
		<center>
			<x-m>0.0</x-m>
			<y-m>-0.044</y-m>
			<z-m>0.051</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>rpm.R</object-name>
		<property>engines/engine[1]/rpm</property>
		<factor>2</factor>
		<center>
			<x-m>0.0</x-m>
			<y-m>0.046</y-m>
			<z-m>0.051</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

 <animation>
	 <type>textranslate</type>
	 <object-name>rpm1.L1</object-name>
	 <property>engines/engine[0]/rpm</property>
	 <factor>0.001</factor>
	 <step>100.0</step>
	 <axis>
		 <x>0</x>
		 <y>-1</y>
		 <z>0</z>
	 </axis>
 </animation>

 <animation>
	 <type>textranslate</type>
	 <object-name>rpm1.L2</object-name>
	 <property>engines/engine[0]/rpm</property>
	 <factor>0.01</factor>
	 <step>10.0</step>
	 <axis>
		 <x>0</x>
		 <y>-1</y>
		 <z>0</z>
	 </axis>
 </animation>
 
 <animation>
	 <type>textranslate</type>
	 <object-name>rpm1.L3</object-name>
	 <property>engines/engine[0]/rpm</property>
	 <factor>0.1</factor>
	 <step>1.0</step>
	 <axis>
		 <x>0</x>
		 <y>-1</y>
		 <z>0</z>
	 </axis>
 </animation>

 <animation>
	 <type>textranslate</type>
	 <object-name>rpm1.R1</object-name>
	 <property>engines/engine[1]/rpm</property>
	 <factor>0.001</factor>
	 <step>100.0</step>
	 <axis>
		 <x>0</x>
		 <y>-1</y>
		 <z>0</z>
	 </axis>
 </animation>

 <animation>
	 <type>textranslate</type>
	 <object-name>rpm1.R2</object-name>
	 <property>engines/engine[1]/rpm</property>
	 <factor>0.01</factor>
	 <step>10.0</step>
	 <axis>
		 <x>0</x>
		 <y>-1</y>
		 <z>0</z>
	 </axis>
 </animation>
 
 <animation>
	 <type>textranslate</type>
	 <object-name>rpm1.R3</object-name>
	 <property>engines/engine[1]/rpm</property>
	 <factor>0.1</factor>
	 <step>1.0</step>
	 <axis>
		 <x>0</x>
		 <y>-1</y>
		 <z>0</z>
	 </axis>
 </animation>


	<text>
	 <name>norm</name>
	  <font>lcd.txf</font>
    <character-size type="double">0.013</character-size>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <heading-deg>0</heading-deg>
      <roll-deg>90</roll-deg>
      <x-m>0.018</x-m>
      <y-m>0.079</y-m>
      <z-m>0.065</z-m>
    </offsets>
	  <type type="string">literal</type>
	  <text type="string">NORM</text>
	</text>
	<text>
	 <name>fuel</name>
	  <font>lcd.txf</font>
    <character-size type="double">0.013</character-size>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <heading-deg>0</heading-deg>
      <roll-deg>90</roll-deg>
      <x-m>0.018</x-m>
      <y-m>-0.056</y-m>
      <z-m>-0.078</z-m>
    </offsets>
	  <type type="string">literal</type>
	  <text type="string">FUEL</text>
	</text>
	<text>
	 <name>engine</name>
	  <font>lcd.txf</font>
    <character-size type="double">0.013</character-size>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <heading-deg>0</heading-deg>
      <roll-deg>90</roll-deg>
      <x-m>0.018</x-m>
      <y-m>-0.034</y-m>
      <z-m>-0.078</z-m>
    </offsets>
	  <type type="string">literal</type>
	  <text type="string">ENG</text>
		<draw-boundingbox type="bool">true</draw-boundingbox>
	</text>
	<text>
	 <name>hydraulic</name>
	  <font>lcd.txf</font>
    <character-size type="double">0.013</character-size>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <heading-deg>0</heading-deg>
      <roll-deg>90</roll-deg>
      <x-m>0.018</x-m>
      <y-m>-0.01</y-m>
      <z-m>-0.078</z-m>
    </offsets>
	  <type type="string">literal</type>
	  <text type="string">HYD</text>
	</text>
	<text>
	 <name>waypoint</name>
	  <font>lcd.txf</font>
    <character-size type="double">0.013</character-size>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <heading-deg>0</heading-deg>
      <roll-deg>90</roll-deg>
      <x-m>0.018</x-m>
      <y-m>0.015</y-m>
      <z-m>-0.078</z-m>
    </offsets>
	  <type type="string">literal</type>
	  <text type="string">WPT</text>
	</text>


	<text>
	 <name>frequency</name>
	  <font>lcd.txf</font>
    <character-size type="double">0.013</character-size>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <heading-deg>0</heading-deg>
      <roll-deg>90</roll-deg>
      <x-m>0.018</x-m>
      <y-m>.039</y-m>
      <z-m>-0.078</z-m>
    </offsets>
	  <type type="string">literal</type>
	  <text type="string">FREQ</text>

	</text>


<animation>
  <type>material</type>
  <object-name>fuel</object-name>
  <object-name>engine</object-name>
  <object-name>hydraulic</object-name>
  <object-name>waypoint</object-name>
  <object-name>frequency</object-name>
  <object-name>norm</object-name>
  <diffuse>
    <red>1.0</red>
    <green>1.0</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>1.0</red>
    <green>1.0</green>
    <blue>0.0</blue>
    <factor-prop>/instrumentation/illumination/led-brightness</factor-prop>
  </emission>
</animation>

</PropertyList>
