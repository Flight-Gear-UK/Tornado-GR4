<?xml version="1.0"?>

<PropertyList>

 <path>A-10-rwr.ac</path>


  <animation> 
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.05</red>
        <green>0.20</green>
        <blue>0.05</blue>
      </emission>
  </animation>

<!-- lighting -->   
<animation> 
   <object-name>mount</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.45</red>
        <green>0.34</green>
        <blue>0.18</blue>
      </emission>
  </animation>

  <animation> 
   <object-name>button</object-name>
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.25</red>
        <green>0.18</green>
        <blue>0.09</blue>
      </emission>
  </animation>

  <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>10</max-m>
 </animation>  



</PropertyList>
