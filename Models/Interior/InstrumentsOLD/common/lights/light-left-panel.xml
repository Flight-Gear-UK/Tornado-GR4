<?xml version="1.0"?>

	<PropertyList>

	<path>light-left-panel.ac</path>

	<!-- collision-switch -->
	<animation>
		<object-name>collision-switch</object-name>
		<type>rotate</type>
		<property>controls/lighting/beacon</property>
		<interpolation>
			<entry><ind>0</ind><dep>-10</dep></entry>
			<entry><ind>1</ind><dep>25</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.0549</x-m>
			<y-m>-0.0429</y-m>
			<z-m>-0.0047</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- taxi/probe-switch -->
	<animation>
		<object-name>taxi-probe-switch</object-name>
		<type>rotate</type>
		<property>controls/lighting/taxi-light</property>
		<interpolation>
			<entry><ind>0</ind><dep>-10</dep></entry>
			<entry><ind>1</ind><dep>25</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.0146</x-m>
			<y-m>-0.0423</y-m>
			<z-m>-0.0010</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

